    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/googleAPIS/**",
        "function": "api"
      }, {
        "source": "/googleAPIS/update-samp-design/**",
        "function": "api"
      }, {
        "source": "/googleAPIS/update-samp-sheet/**",
        "function": "api"
      }, {
        "source": "/googleAPIS/getAuth/**",
        "function": "api"
      }, {
